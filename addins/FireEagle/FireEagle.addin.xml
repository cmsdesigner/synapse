<Addin id        = "FireEagle"
	   namespace = "Synapse"
	   name      = "FireEagle"
	   author    = "Eric Butler"
	   copyright = "GPLv3"
	   version   = "0.1">

	<Dependencies>
		<Addin id="Services" version="0.1" />
		<Addin id="Xmpp" version="0.1" />
		<Addin id="UI" version="0.1" />
		<Addin id="QtClient" version="0.1" />
	</Dependencies>

	<Extension path="/Synapse/UI/AddinPreferences">
		<AddinPreferencesFactory class="Synapse.Addins.FireEagle.FireEagleAddinPreferencesFactory" />
	</Extension>
	
	<Extension path="/Synapse/ServiceManager/Service">
		<Service class="Synapse.Addins.FireEagle.FireEagleService" />
	</Extension>
	
	<Extension path="/Synapse/UI/ResourceProviders">
		<ResourceProvider id="fireeagle"/>
	</Extension>
</Addin>
